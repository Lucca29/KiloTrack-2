{
  "expo": {
    "name": "KiloTrack",
    "slug": "kilotrack",
    "version": "14",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kilotrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "15",
      "bundleIdentifier": "com.votrecompany.cartracking",
      "deploymentTarget": "16.0",
      "config": {
        "usesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#9381FF"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.votrecompany.kilotrack"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5d5dc5b5-f96d-41d1-9339-3f896b1499db"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#9381FF",
      "translucent": false
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#9381FF"
    }
  }
}